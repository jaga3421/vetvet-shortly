<div class="card-graph row">

    {{#each @content as |card|}}
        <div class="col-md-4 col-sm-12">
            <div class="card">
                <div class="card-icon">
                    <img src={{card.icon}} />
                </div>
                <div class="card-title">
                    {{card.title}}
                </div>
                <p>
                    {{card.sub-text}}
                </p>
            </div>
        </div>
    {{/each}}
</div>